{
  "name": "@typeofweb/server",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "repository": "git://github.com/typeofweb/server",
  "bugs": {
    "url": "https://github.com/typeofweb/server/issues"
  },
  "homepage": "https://github.com/typeofweb/server#readme",
  "author": "Micha≈Ç Miszczyszyn - Type of Web <hi@typeofweb.com> (https://typeofweb.com/)",
  "license": "MIT",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "package.json",
    "dist",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "server",
    "api"
  ],
  "dependencies": {
    "@typeofweb/schema": "0.8.0-5",
    "@types/express": "4.17.12",
    "@types/supertest": "2.0.11",
    "express": "4.17.1",
    "stoppable": "1.1.0",
    "supertest": "6.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@tsconfig/node12": "1.0.8",
    "@tsconfig/node14": "1.0.1",
    "@typeofweb/eslint-plugin": "0.1.10",
    "@types/jest": "26.0.23",
    "@types/node": "12",
    "@types/stoppable": "1.1.1",
    "all-contributors-cli": "6.20.0",
    "eslint": "7.28.0",
    "fast-check": "2.16.0",
    "globby": "11.0.3",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "rollup": "2.51.2",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-license": "2.5.0",
    "rollup-plugin-prettier": "2.1.0",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tsd": "https://github.com/typeofweb/tsd#pkg",
    "tslib": "2.3.0",
    "typescript": "4.3.2"
  },
  "scripts": {
    "prejest": "yarn build",
    "jest": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test": "yarn jest --detectOpenHandles --forceExit --coverage",
    "test:dev": "yarn jest --watch",
    "build": "rimraf dist && rollup --config",
    "build:watch": "rollup --config --watch",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "ts-node": "nodemon -w dist -e js -x 'node --loader ts-node/esm'"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn jest --passWithNoTests",
      "yarn eslint --fix",
      "yarn prettier --write"
    ],
    "**/*.{md,js,json}": [
      "yarn prettier --write"
    ]
  }
}
